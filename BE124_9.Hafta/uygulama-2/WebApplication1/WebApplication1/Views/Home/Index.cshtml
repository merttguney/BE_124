@{
    ViewData["Title"] = "Home Page";
    List<Product>? products = ViewData["Products"] as List<Product>;
}

<div class="text-center">
    <h1>Product List</h1>
    <ul>
        @if (products is not null)
        {
            @foreach (var product in products)
            {
                <li>@product.Name - @product.Price</li>   
            }
        }
    </ul>
</div>
